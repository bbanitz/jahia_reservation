<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <userregistration j:dependencies="default"
                        j:modulePriority="0"
                        j:moduleType="module"
                        j:title="Jahia User Registration"
                        jcr:mixinTypes="jmix:hasExternalProviderExtension"
                        jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-userregistration-2.0.5">
                    <src jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                        <main jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                            <java jcr:mixinTypes="jmix:sourceControl"
                        jcr:primaryType="jnt:javaPackageFolder">
                                <org jcr:mixinTypes="jmix:sourceControl"
                          jcr:primaryType="jnt:javaPackageFolder">
                                    <jahia jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                                        <modules jcr:mixinTypes="jmix:sourceControl"
                                    jcr:primaryType="jnt:javaPackageFolder">
                                            <userregistration jcr:mixinTypes="jmix:sourceControl"
                                                jcr:primaryType="jnt:javaPackageFolder">
                                                <actions jcr:mixinTypes="jmix:sourceControl"
                                          jcr:primaryType="jnt:javaPackageFolder">
                                                    <ConfirmationReservation.java jcr:mixinTypes="jmix:sourceControl"
                                                                  jcr:primaryType="jnt:javaSourceFile"/>
                                                    <NewReservation.java jcr:mixinTypes="jmix:sourceControl"
                                                         jcr:primaryType="jnt:javaSourceFile"/>
                                                </actions>
                                            </userregistration>
                                        </modules>
                                    </jahia>
                                </org>
                            </java>
                            <resources jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                                <jnt_reservations jcr:mixinTypes="jmix:sourceControl"
                                       jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:mixinTypes="jmix:sourceControl"
                              jcr:primaryType="jnt:templateTypeFolder">
                                        <reservations.input.jsp jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:viewFile"/>
                                        <reservations.jsp jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_reservations>
                                <jnt_uneReservation jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:primaryType="jnt:templateTypeFolder">
                                        <uneReservation.jsp jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_uneReservation>
                                <mails jcr:primaryType="jnt:folder">
                                    <templates jcr:primaryType="jnt:folder">
                                        <newReservation.vm jcr:primaryType="jnt:editableFile"/>
                                    </templates>
                                </mails>
                                <META-INF jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:metaInfFolder">
                                    <definitions.cnd jcr:mixinTypes="jmix:sourceControl"
                                         jcr:primaryType="jnt:definitionFile"/>
                                </META-INF>
                            </resources>
                        </main>
                    </src>
                </sources>
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                </templates>
                
            </userregistration>
    </modules>
</content>